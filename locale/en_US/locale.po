msgid "plugins.generic.adminNotificationManager.displayName"
msgstr "Administrator Notification Manager"

msgid "plugins.generic.adminNotificationManager.disableAllNotifications"
msgstr "Disable Active Notifications"

msgid "grid.action.plugins.generic.adminNotificationManager.disableAllNotificationsTooltip"
msgstr "Disable Active Notifications"

msgid "plugins.generic.adminNotificationManager.description"
msgstr "<p>Enable this plugin to disable email notifications in the settings for newly created journals. This plugin also allows an administrator to disable email notifications for all existing journals.</p>"

msgid "plugins.generic.adminNotificationManager.instructions"
msgstr "<p>Click OK to disable all email notification settings for all administrative-level users for all journals. Existing settings cannot be recovered after this action, but each user may go to the notification settings of each journal individually and change their notification settings manually.</p>"
